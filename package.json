    "prop-types": "15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.0.0",
    "reflexbox": "^3.0.1",
    "reselect": "3.0.1",
    "sanitize.css": "4.1.0",
    "styled-components": "^3.4.2",
    "warning": "^4.0.1",
    "whatwg-fetch": "2.0.4"
